<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.ConfirmationController">
   <children>
      <Label layoutX="44.0" layoutY="20.0" text="Вы уверены что хотите скачать файл: ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="fileName" layoutX="44.0" layoutY="57.0" prefHeight="45.0" prefWidth="342.0" />
      <Button fx:id="cancelButton" layoutX="254.0" layoutY="155.0" mnemonicParsing="false" onAction="#cancelAction" text="Отмена" />
      <Button fx:id="uploadButton" layoutX="317.0" layoutY="155.0" mnemonicParsing="false" onAction="#uploadAction" text="Скачать" />
      <Label fx:id="fileSize" layoutX="44.0" layoutY="122.0" text="Размер файла: ">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
